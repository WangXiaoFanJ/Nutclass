        <LinearLayout
            android:id="@+id/ll_all_info_02"
            android:visibility="gone"
            android:background="@drawable/edu_map_info_bg"
            android:layout_width="match_parent"
            android:layout_height="96dp"
            android:layout_marginLeft="17dp"
            android:layout_marginRight="17dp"
            android:layout_marginBottom="15dp"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_marginLeft="@dimen/common_10"
                android:layout_width="0dp"
                android:layout_weight="5"
                android:layout_height="match_parent"
                android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_school_name_02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="泰迪斯烘焙课程"
                        android:textSize="@dimen/font_12"
                        android:layout_marginTop="@dimen/common_7"
                        android:textColor="@color/color_80"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginTop="@dimen/common_6">
                    <TextView
                        android:id="@+id/tv_distance_02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1000米"
                        android:textColor="#b8b8b8"
                        android:textSize="@dimen/font_10"/>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="10dp"
                        android:background="#b8b8b8"
                        android:layout_marginLeft="@dimen/common_10"
                        android:layout_marginRight="@dimen/common_10"/>
                    <TextView
                        android:id="@+id/tv_school_address_02"
                        android:singleLine="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#b8b8b8"
                        android:textSize="@dimen/font_10"
                        android:text="天地家园200号"/>
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_back_money_02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="此机构未加入课比课"
                    android:layout_marginTop="@dimen/common_6"
                    android:textColor="#969696"
                    android:textSize="@dimen/font_11"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#f1f1f1"
                    android:layout_marginTop="@dimen/common_6"
                    />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="点击按钮，向高价位课程说再见"
                        android:textSize="@dimen/font_11"
                        android:textColor="@color/home_nav_txt_ed"/>
                </LinearLayout>

            </LinearLayout>
            <ImageView
                android:id="@+id/btn_want_discount"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:src="@drawable/req_discount_btn"/>

        </LinearLayout>



