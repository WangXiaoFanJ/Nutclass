            android:id="@+id/btn_lock_position"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_lock_position"
            android:layout_marginLeft="@dimen/common_17"
            android:layout_centerVertical="true"
            android:paddingTop="210dp"
           />
        <LinearLayout
            android:id="@+id/ll_all_info"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_marginLeft="17dp"
            android:layout_marginRight="17dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="15dp"
            android:background="@drawable/edu_map_info_bg"
            >
            <LinearLayout
                android:id="@+id/ll_card_school"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="4dp"
                android:paddingBottom="4dp"
                android:background="@color/color_white"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_school_icon"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:scaleType="fitXY"
                    android:layout_marginLeft="4dp" />
                <LinearLayout
                    android:layout_marginLeft="6dp"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="vertical">
                        <TextView
                        android:id="@+id/tv_school_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="泰迪斯烘焙课程"
                        android:textSize="@dimen/font_12"
                        android:layout_marginTop="3dp"
                        android:textColor="@color/color_80"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:layout_marginTop="@dimen/common_5">
                        <ImageView
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_marginRight="@dimen/common_2"
                            android:src="@drawable/icon_distance"/>
                        <TextView
                            android:id="@+id/tv_distance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1000米"
                            android:textColor="#b8b8b8"
                            android:textSize="@dimen/font_10"/>
                        <View
                            android:layout_width="1dp"
                            android:layout_height="10dp"
                            android:background="#b8b8b8"
                            android:layout_marginLeft="@dimen/common_10"
                            android:layout_marginRight="@dimen/common_10"/>
                        <TextView
                            android:id="@+id/tv_school_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#b8b8b8"
                            android:textSize="@dimen/font_10"
                            android:singleLine="true"
                            android:text="天地家园200号"/>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_back_money"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="购课最高可减4320元"
                        android:layout_marginTop="@dimen/common_5"
                        android:textColor="#dd163f"
                        android:textSize="@dimen/font_11"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:background="#f1f1f1"
                android:gravity="center_vertical">
                <LinearLayout
                    android:id="@+id/ll_cat"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_school_cate"
                        android:layout_marginRight="@dimen/common_5"/>
                    <TextView
                        android:id="@+id/tv_cat_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="其他"
                        android:textColor="@color/color_80"
                        android:textSize="@dimen/font_11"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_tel"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <View
                        android:layout_width="1dp"
                        android:layout_height="12dp"
                        android:background="#b8b8b8"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_call"
                            android:layout_marginRight="@dimen/common_5"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="拨打电话"
                            android:textColor="@color/color_80"
                            android:textSize="@dimen/font_11"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_course_detail"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                     >
                    <View
                        android:layout_width="1dp"
                        android:layout_height="12dp"
                        android:background="#b8b8b8"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_show_detail"
                        android:layout_marginRight="@dimen/common_5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="查看详情"
                        android:textColor="@color/color_80"
                        android:textSize="@dimen/font_11"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
