    <!--新版注册界面-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_44"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_50"
        android:background="@color/color_white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/edit_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_10"
            android:background="@null"
            android:hint="请输入您的手机号"
            android:textColorHint="#a9a9a9"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_get_verifycode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="@dimen/common_9"
            android:layout_marginRight="@dimen/common_5"
            android:text="获取验证码"
            android:textColor="@color/login_orange" />

        <View
            android:layout_width="1dp"
            android:layout_height="@dimen/common_16"
            android:layout_toLeftOf="@+id/tv_get_verifycode"
            android:background="#a9a9a9" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_15"
        android:background="#999999" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_44"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_18"
        android:background="@color/color_white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/edit_verifycode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_10"
            android:background="@null"
            android:hint="请输入验证码"
            android:textColorHint="#a9a9a9"
            android:textSize="16sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_15"
        android:background="#999999" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_8">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common_15"
            android:text="收不到短信验证码？试试" />

        <TextView
            android:id="@+id/tv_get_verifycode_sound"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="语音验证"
            android:textColor="@color/login_orange" />
    </LinearLayout>

    <Button
        android:id="@+id/tv_register_user"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_35"
        android:layout_marginLeft="@dimen/common_20"
        android:layout_marginRight="@dimen/common_20"
        android:layout_marginTop="@dimen/common_25"
        android:background="@drawable/shape_login_btn"
        android:text="完       成"
        android:textColor="#ffffff" />
</LinearLayout>